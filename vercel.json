{
  "buildCommand": "astro build",
  "devCommand": "astro dev",
  "framework": "astro",
  "installCommand": "npm install",
  "build": {
    "env": {
      "VERCEL_FORCE_NO_BUILD_CACHE": "1"
    }
  },
  "functions": {
    "api/*.ts": {
      "runtime": "nodejs20.x"
    },
    "src/pages/**/*.astro": {
      "runtime": "nodejs20.x"
    },
    "_image.astro": {
      "runtime": "nodejs20.x"
    }
  }
}
